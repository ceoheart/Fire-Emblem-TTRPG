                    damagesupportmod23 = 0;
                    defensesupportmod23 = 0.5;
                    hitsupportmod23 = 2.5;
                    avosupportmod23 = 0;
                    critsupportmod23 = 2.5;
                    critavosupportmod23 = 2.5;
                    break;
                case "Earth23":
                    damagesupportmod23 = 0;
                    defensesupportmod23 = 0.5;
                    hitsupportmod23 = 0;
                    avosupportmod23 = 2.5;
                    critsupportmod23 = 2.5;
                    critavosupportmod23 = 2.5;
                    break;
                case "Dark23":
                    damagesupportmod23 = 0;
                    defensesupportmod23 = 0;
                    hitsupportmod23 = 0.5;
                    avosupportmod23 = 2.5;
                    critsupportmod23 = 2.5;
                    critavosupportmod23 = 2.5;
                    break;
                case "Light23":
                    damagesupportmod23 = 0.5;
                    defensesupportmod23 = 0.5;
                    hitsupportmod23 = 2.5;
                    avosupportmod23 = 0;
                    critsupportmod23 = 2.5;
                    critavosupportmod23 = 0;
                    break;
                case "Anima23":
                    damagesupportmod23 = 0.5;
                    defensesupportmod23 = 0.5;
                    hitsupportmod23 = 0;
                    avosupportmod23 = 2.5;
                    critsupportmod23 = 0;
                    critavosupportmod23 = 2.5;
                    break;
            }
            setAttrs({
                "damagesupportmod23": damagesupportmod23,
                "defensesupportmod23": defensesupportmod23,
                "hitsupportmod23": hitsupportmod23,
                "avosupportmod23": avosupportmod23,
                "critsupportmod23": critsupportmod23,
                "critavosupportmod23": critavosupportmod23
            });
        });
    });
</script>
