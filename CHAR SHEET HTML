                    <script type="text/worker">
    on("sheet:opened change:affinity24", function () {
        getAttrs(["affinity24"], function (values) {
            let affinity24 = values.affinity24;
            let damagesupportmod24 = 0;
            let defensesupportmod24 = 0;
            let hitsupportmod24 = 0;
            let critsupportmod24 = 0;
            let avosupportmod24 = 0;
            let critavosupportmod24 = 0;
    
            switch (affinity24) {
                case "Fire24":
                    damagesupportmod24 = 0.5;
                    defensesupportmod24 = 0;
                    hitsupportmod24 = 2.5;
                    avosupportmod24 = 2.5;
                    critsupportmod24 = 2.5;
                    critavosupportmod24 = 0;
                    break;
                case "Ice24":
                    damagesupportmod24 = 0;
                    defensesupportmod24 = 0.5;
                    hitsupportmod24 = 2.5;
                    avosupportmod24 = 2.5;
                    critsupportmod24 = 0;
                    critavosupportmod24 = 2.5;
                    break;
                case "Wind24":
                    damagesupportmod24 = 0.5;
                    defensesupportmod24 = 0;
                    hitsupportmod24 = 2.5;
                    avosupportmod24 = 0;
                    critsupportmod24 = 2.5;
                    critavosupportmod24 = 2.5;
                    break;
                case "Thunder24":
                    damagesupportmod24 = 0;
                    defensesupportmod24 = 0.5;
                    hitsupportmod24 = 2.5;
                    avosupportmod24 = 0;
                    critsupportmod24 = 2.5;
                    critavosupportmod24 = 2.5;
                    break;
                case "Earth24":
                    damagesupportmod24 = 0;
                    defensesupportmod24 = 0.5;
                    hitsupportmod24 = 0;
                    avosupportmod24 = 2.5;
                    critsupportmod24 = 2.5;
                    critavosupportmod24 = 2.5;
                    break;
                case "Dark24":
                    damagesupportmod24 = 0;
                    defensesupportmod24 = 0;
                    hitsupportmod24 = 0.5;
                    avosupportmod24 = 2.5;
                    critsupportmod24 = 2.5;
                    critavosupportmod24 = 2.5;
                    break;
                case "Light24":
                    damagesupportmod24 = 0.5;
                    defensesupportmod24 = 0.5;
                    hitsupportmod24 = 2.5;
                    avosupportmod24 = 0;
                    critsupportmod24 = 2.5;
                    critavosupportmod24 = 0;
                    break;
                case "Anima24":
                    damagesupportmod24 = 0.5;
                    defensesupportmod24 = 0.5;
                    hitsupportmod24 = 0;
                    avosupportmod24 = 2.5;
                    critsupportmod24 = 0;
                    critavosupportmod24 = 2.5;
                    break;
            }
            setAttrs({
                "damagesupportmod24": damagesupportmod24,
                "defensesupportmod24": defensesupportmod24,
                "hitsupportmod24": hitsupportmod24,
                "avosupportmod24": avosupportmod24,
                "critsupportmod24": critsupportmod24,
                "critavosupportmod24": critavosupportmod24
            });
        });
    });
</script> 
